{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "strict": "global",
  "rules": {
    "no-param-reassign": [0, {"props": false}],
    "strict": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "max-len": [1, 80, 2, {"ignoreUrls": true}]
  }
}
